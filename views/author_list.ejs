<html>
  <%- include('header') %>
  <body>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-2"><%- include('navbar')%></div>
        <div class="col-sm-10">
          <h1><%=title%></h1>
          <% if (author_list.length) { %>
          <ul>
            <% author_list.forEach(function(author) { %>
            <li>
              <a href="<%= author.url %>"><%= author.name %></a>
              (<%= author.lifespan %>)
            </li>
            <% }) %>
          </ul>
          <% } else { %>
          <p>There are no authors.</p>
          <% } %>
        </div>
      </div>
    </div>
  </body>
</html>
